{
    "manifest_version": 2,
    "name": "Sample youtube downloader",
    "description": "Sample yotube downloader",
    "version": "1.0",
    "browser_action": {
        "default_icon": "click.png",
        "default_title": "Simle extension"
    },
    "permissions": [
      "activeTab"
    ],

	"icons": { "16": "click.png",
             "48": "click.png",
             "128": "click.png" },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
